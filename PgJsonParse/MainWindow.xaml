            <StackPanel Orientation="Horizontal">
                <TextBlock Text="{Binding FriendlyName}" TextWrapping="Wrap" Width="300" TextAlignment="Right"/>
                <TextBlock>:</TextBlock>
                <TextBlock Text="{Binding FriendlyEffect}" Width="150" Margin="5,0,0,0"/>
