                    <TextBox Text="{Binding WarningText, Mode=OneWay}" IsReadOnly="True" VerticalScrollBarVisibility="Visible" BorderBrush="{x:Null}" BorderThickness="0" Margin="10,10,10,10"/>
                </DockPanel>
            </TabItem>
